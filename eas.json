{
    "cli": {
        "version": ">= 5.6.0"
    },
    "build": {
        "development-simulator": {
            "env": {
                "APP_VARIANT": "simulator"
            },
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "simulator": true
            }
        },
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "development"
            },
            "android": {
                "buildType": "apk"
            },
            "ios": {
                "resourceClass": "m-medium"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "preview"
            },
            "android": {
                "buildType": "apk"
            },
            "ios": {
                "resourceClass": "m-medium"
            }
        },
        "release": {
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "release"
            },
            "ios": {
                "resourceClass": "m-medium"
            }
        }
    },
    "submit": {
        "beta": {
            "ios": {
                "ascAppId": "1670034414"
            }
        },
        "release": {
            "ios": {
                "ascAppId": "6446094615"
            }
        }
    }
}
